{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "PostgresOptions": {
    "ConnectionString": "Server=localhost;Port=5432;Database=reviews;User Id=postgres;Password=postgres;Include Error Detail=true",
    "UseInMemory": false
  },
  "SemanticKernelOptions":{
    "ChatModel": "gpt-4.1",
    "ChatDeploymentName": "gpt-4.1",
    "EmbeddingDeploymentName": "text-embedding-3-small",
    "EmbeddingModel": "text-embedding-3-small",
    "ChatProviderType": "Azure",
    "EmbeddingProviderType": "Azure",
    "Temperature": 0.2,
    "SearchThreshold": 0.4
  },
  // will overiide by aspire if we use apire
  "Services": {
    "catalogs-api": {
      "https": [
        "https://localhost:5001"
      ],
      "http": [
        "http://localhost:5000"
      ]
    }
  }
}
